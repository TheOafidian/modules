params {
    test_data_metabuli = 'https://raw.githubusercontent.com/nf-core/test-datasets/metabuli'
}    
process {

    publishDir = { "${params.outdir}/${task.process.tokenize(':')[-1].tokenize('_')[0].toLowerCase()}" }
    withLabel:process_medium {
        cpus   = 6
        memory = 36.GB
        time   = 8.h
    }
}